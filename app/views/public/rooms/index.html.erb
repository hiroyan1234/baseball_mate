<% @enters.each do |enter| %>
<% if enter.user == current_user %>
  <%= link_to room_path(enter.room) do %>
  <div class="card">
    <div class="card-body">
      <div>
        <div class="d-flex">
          <div cass="chat-avatar">
            <object>
            </object>
          </div>
          <div class="chat-user-name">
            <%= enter.user.last_name %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>
<% end %>
<% end %>
